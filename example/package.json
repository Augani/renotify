{
  "name": "renotify-example",
  "homepage": ".",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "tailwind:css": "tailwind build src/css/tailwind.gen.css -c  tailwind.config.js -o src/css/tailwind.css",
    "start": "yarn tailwind:css && node ../node_modules/react-scripts/bin/react-scripts.js start",
    "build": "yarn tailwind:css && node ../node_modules/react-scripts/bin/react-scripts.js build",
    "test": "node ../node_modules/react-scripts/bin/react-scripts.js test",
    "eject": "node ../node_modules/react-scripts/bin/react-scripts.js eject"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "react": "link:../node_modules/react",
    "react-dom": "link:../node_modules/react-dom",
    "react-scripts": "link:../node_modules/react-scripts",
    "renotify-react": "link:..",
    "tailwindcss": "^1.8.10"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
